[configuration]

entry_symbol = "kuzugd_init"
compatibility_minimum = "4.4"

[libraries]
; Relative paths ensure that our GDExtension can be placed anywhere in the project directory.
macos.single.debug = "res://addons/KuzuGD/bin/macos/libKuzuGD.macos.template_debug.dylib"
macos.double.debug = "res://addons/KuzuGD/bin/macos/libKuzuGD.macos.template_debug.double.dylib"
macos.single.release = "res://addons/KuzuGD/bin/macos/libKuzuGD.macos.template_release.dylib"
macos.double.release = "res://addons/KuzuGD/bin/macos/libKuzuGD.macos.template_debug.double.dylib"

windows.x86_64.single.debug = "res://addons/KuzuGD/bin/windows/KuzuGD.windows.template_debug.x86_64.dll"
windows.x86_64.double.debug = "res://addons/KuzuGD/bin/windows/KuzuGD.windows.template_debug.double.x86_64.dll"
windows.x86_64.single.release = "res://addons/KuzuGD/bin/windows/KuzuGD.windows.template_release.x86_64.dll"
windows.x86_64.double.release = "res://addons/KuzuGD/bin/windows/KuzuGD.windows.template_release.double.x86_64.dll"

linux.x86_64.single.debug = "res://addons/KuzuGD/bin/linux/libKuzuGD.linux.template_debug.x86_64.so"
linux.x86_64.double.debug = "res://addons/KuzuGD/bin/linux/libKuzuGD.linux.template_debug.x86_64.double.so"
linux.x86_64.single.release = "res://addons/KuzuGD/bin/linux/libKuzuGD.linux.template_release.x86_64.so"
linux.x86_64.double.release = "res://addons/KuzuGD/bin/linux/libKuzuGD.linux.template_release.x86_64.double.so"

[dependencies]

macos.single.debug = "res://addons/KuzuGD/bin/macos/kuzu/libkuzu.dylib"
